<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="external_layout">
            <!-- Multicompany -->
            <t t-if="not o" t-set="o" t-value="doc"/>

            <t t-if="not company">
                <!-- Multicompany -->
                <t t-if="company_id">
                    <t t-set="company" t-value="company_id"/>
                </t>
                <t t-elif="o and 'company_id' in o">
                    <t t-set="company" t-value="o.company_id.sudo()"/>
                </t>
                <t t-else="else">
                    <t t-set="company" t-value="res_company"/>
                </t>
            </t>

            <t t-call="0043_account.external_layout_header"/>
            <t t-raw="0" />
            <t t-call="0043_account.external_layout_footer"/>
        </template>

        <template id="external_layout_header">
            <div class="header">
                <div class="row">
                    <div class="col-6" style="padding:0;margin:0;">
                        <div class="row" t-if="o.type == 'out_invoice'">
                            <div class="col-3">
                                <img src='/0043_account/static/src/img/bcp_logo.jpeg' style="max-height: 60px;"/>
                            </div>
                            <div class="col-9">
                                <div class="row">
                                    <div class="col">
                                        <div class="m-0 p-0" style="font-size:10px;"><strong>FAKTUR PENJUALAN</strong></div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col">
                                        <div class="m-0 p-0" style="font-size:10px;" t-esc="company.name"/>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col">
                                        <div class="m-0 p-0" style="font-size:10px;" t-esc="company.street"/>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col">
                                        <div class="m-0 p-0" style="font-size:10px;">NPWP :  <t t-esc="company.vat"/></div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col" >
                                        <div class="m-0 p-0" style="font-size:10px">
                                            WA1 : <t t-esc="company.wa1"/> / WA2 : <t t-esc="company.wa2"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col">
                                        <div class="m-0 p-0" style="font-size:10px">
                                            <t t-esc="o.create_uid.name"/> P-<t t-esc="o.printed_number"/> / 
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row" t-if="o.type == 'in_invoice'">
                            <div class="col-3">
                                <div class="row">
                                   <span><![CDATA[&nbsp;]]></span>
                                </div>
                                <img src='/0043_account/static/src/img/bcp_logo.jpeg' style="max-height: 60px;"/>
                            </div>
                            <div class="col-9">
                                <div class="row">
                                    <span><strong>FAKTUR PEMBELIAN</strong></span>
                                </div>
                                <div class="row">
                                    <span t-esc="company.name"/>
                                </div>
                                <div class="row">
                                    <span style="font-size:10px" t-esc="company.street"/>
                                </div>
                                <div class="row">
                                    <span style="font-size:10px">NPWP : </span><span style="font-size:10px" t-esc="company.vat"/>
                                </div>
                                <div class="row">
                                    <span style="font-size:10px">WA1 : </span>
                                    <span style="font-size:10px" t-esc="company.wa1"/>
                                    <span style="font-size:10px"> / WA2 : </span>
                                    <span style="font-size:10px" t-esc="company.wa2"/>
                                </div>
                                <div class="row">
                                    <span style="font-size:10px" t-esc="o.create_uid.name"/>
                                    <span style="font-size:10px"> P-</span><span style="font-size:10px" t-esc="o.printed_number"/> <span style="font-size:10px">/</span> <span style="font-size:10px" class="page"/> <span  style="font-size:10px">of</span> <span style="font-size:10px" class="topage"/>
                                    <span style="font-size:10px">   </span>
                                </div>
                            </div>
                        </div>
                        <div class="row" t-if="o.type == 'out_refund'">
                            <div class="col-3">
                                <div class="row">
                                   <span><![CDATA[&nbsp;]]></span>
                                </div>
                                <img src='/0043_account/static/src/img/bcp_logo.jpeg' style="max-height: 60px;"/>
                            </div>
                            <div class="col-9">
                                <div class="row">
                                    <div class="col-12">
                                        <span><strong><![CDATA[&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;]]>REFUND PENJUALAN</strong></span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-12">
                                        <span><![CDATA[&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;]]></span><span t-esc="company.name"/>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-12">
                                        <span style="font-size:10px"><![CDATA[&nbsp;&nbsp;&nbsp;&nbsp;]]></span>
                                        <span style="font-size:10px" t-esc="o.create_uid.name"/>
                                        <span style="font-size:10px"> P-</span><span style="font-size:10px" t-esc="o.printed_number"/> <span style="font-size:10px">/</span> <span style="font-size:10px" class="page"/> <span  style="font-size:10px">of</span> <span style="font-size:10px" class="topage"/>
                                        <span style="font-size:10px">   </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row" t-if="o.type == 'in_refund'">
                            <div class="col-xs-3">
                                <div class="row">
                                   <span><![CDATA[&nbsp;]]></span>
                                </div>
                                <img src='/0043_account/static/src/img/bcp_logo.jpeg' style="max-height: 60px;"/>
                            </div>
                            <div class="col-xs-9">
                                <div class="row">
                                    <div class="col-xs-12">
                                        <span><strong><![CDATA[&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;]]>REFUND PENJUALAN</strong></span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xs-12">
                                        <span><![CDATA[&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;]]></span><span t-esc="company.name"/>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xs-12">
                                        <span style="font-size:10px"><![CDATA[&nbsp;&nbsp;&nbsp;&nbsp;]]></span>
                                        <span style="font-size:10px" t-esc="o.create_uid.name"/>
                                        <span style="font-size:10px"> P-</span><span style="font-size:10px" t-esc="o.printed_number"/> <span style="font-size:10px">/</span> <span style="font-size:10px" class="page"/> <span  style="font-size:10px">of</span> <span style="font-size:10px" class="topage"/>
                                        <span style="font-size:10px">   </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-6" style="padding:0;margin:0;">
                        <div class="row">
                                <div class="col-4">
                                    <t t-if="o.type == 'out_invoice'">
                                         <div class="m-0 p-0" style="font-size:10px"><strong>Tgl Faktur</strong></div>
                                    </t>
                                    <t t-if="o.type == 'in_invoice'">
                                         <div class="m-0 p-0" style="font-size:10px"><strong>Tgl Faktur</strong></div>
                                    </t>
                                    <t t-if="o.type == 'out_refund'">
                                         <div style="font-size:10px"><strong>Tgl Refund</strong></div>
                                    </t>
                                    <t t-if="o.type == 'in_refund'">
                                         <div style="font-size:10px"><strong>Tgl Refund</strong></div>
                                    </t>
                                </div>
                                <div class="col-8">
                                    <t t-if="o.date_invoice">
                                        <div class="m-0 p-0" style="font-size:10px"  t-field="o.date_invoice"/>
                                    </t>
                                </div>
                            </div>
                        <div class="row">
                                <div class="col-4">
                                    <t t-if="o.type == 'out_invoice'">
                                        <div class="m-0 p-0" style="font-size:10px"><strong>No. Faktur</strong></div>
                                    </t>
                                    <t t-if="o.type == 'in_invoice'">
                                        <div style="font-size:10px"><strong>No. Faktur</strong></div>
                                    </t>
                                    <t t-if="o.type == 'out_refund'">
                                        <div style="font-size:10px"><strong>No. Refund</strong></div>
                                    </t>
                                    <t t-if="o.type == 'in_refund'">
                                        <div style="font-size:10px"><strong>No. Refund</strong></div>
                                    </t>
                                </div>
                                <div class="col-8">
                                    <div class="m-0 p-0" style="font-size:10px">
                                        <t t-esc="o.number"/>
                                        <t t-if="o.sale_order_id">
                                            / <t t-esc="o.sale_order_id.name"/>
                                        </t>
                                        <t t-if="len(o.picking_ids)>0">
                                            / <t t-esc="o.picking_ids[0].name"/>
                                        </t>
                                    </div>
                                </div>
                            </div>
                        <div class="row" t-if="o.type == 'out_refund'">
                                <div class="col-4">
                                    <div class="m-0 p-0" style="font-size:10px"><strong>No. Faktur</strong></div>
                                </div>
                                <div class="col-8">
                                    <div class="m-0 p-0" style="font-size:10px" t-esc="o.origin"/>
                                </div>
                         </div>
                        <div class="row" t-if="o.type == 'in_refund'">
                                <div class="col-4">
                                    <span style="font-size:10px"><strong>No. Faktur</strong></span>
                                </div>
                                <div class="col-8">
                                    <span style="font-size:10px" t-esc="o.origin"/>
                                </div>
                         </div>

                        <div class="row" t-if="o.type == 'out_invoice'">
                                <div class="col-4">
                                    <div class="m-0 p-0" style="font-size:10px"><strong>Pelanggan</strong></div>
                                </div>
                                <div class="col-8">
                                    <div class="m-0 p-0" style="font-size:10px" t-field="o.partner_id.name"/>
                                </div>
                            </div>
                        <div class="row" t-if="o.type == 'out_invoice'">
                                <div class="col-4">
                                    <div class="m-0 p-0" style="font-size:10px"><strong>Alamat Pelanggan</strong></div>
                                </div>
                                <div class="col-8">
                                    <t t-if="o.partner_id.street">
                                        <div class="m-0 p-0" style="font-size:10px" t-esc="o.partner_id.street[0:100]"/>
                                    </t>
                                </div>
                        </div>

                        <div class="row" t-if="o.type == 'out_refund'">
                                <div class="col-4">
                                    <span style="font-size:10px"><strong>Pelanggan</strong></span>
                                </div>
                                <div class="col-8">
                                    <span style="font-size:10px" t-field="o.partner_id.name"/>
                                </div>
                        </div>
                        <div class="row" t-if="o.type == 'out_refund'">
                                <div class="col-4">
                                    <span style="font-size:10px"><strong>Alamat Pelanggan</strong></span>
                                </div>
                                <div class="col-8">
                                    <t t-if="o.partner_id.street">
                                        <span style="font-size:10px" t-esc="o.partner_id.street[0:100]"/>
                                    </t>
                                </div>
                        </div>

                        <div class="row" t-if="o.type == 'in_invoice'">
                                <div class="col-4">
                                    <span style="font-size:10px"><strong>Supplier</strong></span>
                                </div>
                                <div class="col-8">
                                    <span style="font-size:10px" t-field="o.partner_id.name"/>
                                </div>
                            </div>
                        <div class="row" t-if="o.type == 'in_invoice'">
                                <div class="col-4">
                                    <span style="font-size:10px"><strong>Alamat Supplier</strong></span>
                                </div>
                                <div class="col-8">
                                    <t t-if="o.partner_id.street">
                                        <span style="font-size:10px" t-esc="o.partner_id.street[0:100]"/>
                                    </t>
                                </div>
                        </div>

                        <div class="row" t-if="o.type == 'in_refund'">
                                <div class="col-4">
                                    <span style="font-size:10px"><strong>Supplier</strong></span>
                                </div>
                                <div class="col-8">
                                    <span style="font-size:10px" t-field="o.partner_id.name"/>
                                </div>
                            </div>
                        <div class="row" t-if="o.type == 'in_refund'">
                                <div class="col-4">
                                    <span style="font-size:10px"><strong>Alamat Supplier</strong></span>
                                </div>
                                <div class="col-8">
                                    <t t-if="o.partner_id.street">
                                        <span style="font-size:10px" t-esc="o.partner_id.street[0:100]"/>
                                    </t>
                                </div>
                        </div>


                        <div class="row">
                                <div class="col-4">
                                    <div class="m-0 p-0" style="font-size:10px"><strong>Kota / Provinsi</strong></div>
                                </div>
                                <div class="col-8">
                                    <div  class="m-0 p-0" style="font-size:10px"> 
                                        <t t-if="o.partner_id.city">
                                            <t t-esc="o.partner_id.city"/> 
                                        </t>
                                        / 
                                        <t t-if="o.partner_id.state_id">
                                            <t t-esc="o.partner_id.state_id.name"/>
                                        </t>
                                    </div>
                                </div>
                        </div>
                        <div class="row">
                            <div class="col-4">
                                <t t-if="o.type == 'out_invoice'">
                                    <div class="m-0 p-0" style="font-size:10px"><strong>Cara Bayar</strong></div>
                                </t>
                                <t t-if="o.type == 'out_refund'">
                                    <div class="m-0 p-0" style="font-size:10px"><strong>Cara Refund</strong></div>
                                </t>
                            </div>
                            <div class="col-8">
                                <div class="m-0 p-0" style="font-size:10px" t-esc="o.partner_id.res_partner_payment_method_id.name"/>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </template>

        <template id="external_layout_footer">
            <div class="footer">
                <div class="row">
                    <table width="100%">
                        <tr style="border-top: 0.5pt solid black;" cellspacing="0" cellpadding="0">
                            <td style="padding:0;margin:0">
                                
                            </td>
                        </tr>
                    </table>
                </div>
                <div class="row">
                    <div class="col-6">
                        <div class="row">
                            <div class="col-12" style="padding:0;margin:0;">
                                <div  style="font-size:10px">
                                    Terbilang : <t t-esc="o.terbilang"/>  Rupiah
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <span><![CDATA[&nbsp;]]></span>
                        </div>
                        <div class="row">
                            <div class="col-6">
                                <div class="row">
                                    <span style="font-size:10px">
                                        <strong>&#160;Hormat Kami</strong>
                                    </span>
                                </div>
                                <div class="row">
                                    <span><![CDATA[&nbsp;]]></span>
                                </div>
                                <div class="row">
                                    <span><![CDATA[&nbsp;]]></span>
                                </div>
                                <div class="row">
                                    <span style="font-size:10px"><strong>(&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;)</strong></span>
                                </div>
                            </div>
                            <div class="col-6">
                                <div class="row">
                                    
                                    <span style="font-size:10px"><strong>&#160;Penerima</strong></span>
 
                                </div>
                                <div class="row">
                                    <span><![CDATA[&nbsp;]]></span>
                                </div>
                                <div class="row">
                                    <span><![CDATA[&nbsp;]]></span>
                                </div>
                                <div class="row">
                                    <span style="font-size:10px"><strong>(&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;)</strong></span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-6">
                        <div class="row">
                            <div class="col-6">
                                <div class="row">
                                    <div class="col-6"  style="padding:0;margin:0;">
                                        <div style="font-size:10px;">
                                            <strong>Total Barang</strong>
                                        </div>
                                    </div>
                                    <div class="col-6 text-right">
                                        <div style="font-size:10px;">
                                            <strong>
                                                <t t-esc="int(quantity)"/> Item <![CDATA[&nbsp;]]>
                                            </strong>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div style="font-size:10px;">
                                        <![CDATA[&nbsp;]]>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-12" style="padding:0;margin:0;">
                                        <div style="font-size:10px;">
                                            <strong>Keterangan:</strong>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-12" style="padding:0;margin:0;">
                                        <table>
                                            <tr>
                                                <td height="35">
                                                    <div class="m-0 p-0" style="font-size:10px">
                                                        <t t-esc="o.comment"/>
                                                    </div>
                                                </td>
                                            </tr>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <div class="col-6">
                                <div class="row">
                                    <div class="col-6" style="padding:0;margin:0">
                                        <div style="font-size:10px;">
                                            <strong class="text-right">Sub Total</strong>
                                        </div>
                                    </div>
                                    <div class="col-6  text-right" style="padding:0;margin:0">
                                        <div style="font-size:10px;">
                                            <strong>
                                                <t t-esc="'{:,.0f}'.format(int(rowsubtotal)).replace(',', 'X').replace('.', ',').replace('X', '.')"/>
                                            </strong>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    
                                    <div class="col-6" style="padding:0;margin:0">
                                        <!--
                                        <div style="font-size:10px;">
                                            <strong>Potongan</strong>
                                        </div>
                                        -->
                                    </div>
                                    <div class="col-6  text-right" style="padding:0;margin:0">
                                        <div style="font-size:10px;">
                                            <strong>
                                                <t t-esc="'{:,.0f}'.format(int(o.discount_total)).replace(',', 'X').replace('.', ',').replace('X', '.')"/>
                                            </strong>
                                        </div>
                                    </div>
                                    
                                </div>
                                <div class="row">
                                    <div class="col-6" style="padding:0;margin:0">
                                        <div style="font-size:10px;">
                                            <strong>Total</strong>
                                        </div>
                                    </div>
                                    <div class="col-6  text-right" style="padding:0;margin:0">
                                        <div style="font-size:10px;">
                                            <strong>
                                                <t t-esc="'{:,.0f}'.format(int(o.amount_total)).replace(',', 'X').replace('.', ',').replace('X', '.')"/>
                                            </strong>
                                        </div>
                                    </div>
                                </div>
                                <div class="row" t-if="o.type == 'out_invoice'">
                                    <t t-set="payment_total" t-value="0"/>
                                    <t t-foreach="o.payment_ids" t-as="move_line">
                                        <t t-if="move_line.state  == 'valid'">
                                            <t t-set="payment_total" t-value="payment_total + move_line.credit"/>
                                        </t>
                                    </t>
                                    <div class="col-6" style="padding:0;margin:0">
                                        <div style="font-size:10px;">
                                            <strong>Bayar</strong>
                                        </div>
                                    </div>
                                    <div class="col-6 text-right" style="padding:0;margin:0">
                                        <div style="font-size:10px;">
                                            <strong>
                                                <t t-esc="'{:,.0f}'.format(int(payment_total)).replace(',', 'X').replace('.', ',').replace('X', '.')"/>
                                            </strong>
                                        </div>
                                    </div>
                                </div>
                                <div class="row" t-if="o.type == 'out_invoice'">
                                    <div class="col-6" style="padding:0;margin:0">
                                        <div style="font-size:10px;">
                                            <strong>Sisa</strong>
                                        </div>
                                    </div>
                                    <div class="col-6  text-right" style="padding:0;margin:0">
                                        <div style="font-size:10px;">
                                            <strong>
                                                <t t-esc="'{:,.0f}'.format(int(o.residual)).replace(',', 'X').replace('.', ',').replace('X', '.')"/>
                                            </strong>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-12" style="padding:0;margin:0;" t-if="o.type == 'out_invoice'">
                                <ul>
                                    <li style="font-size:9px" t-if="company.other_info_row1">
                                        <div class="m-0 p-0">
                                            <t t-esc="company.other_info_row1"/>
                                        </div>
                                    </li>
                                    <li style="font-size:9px" t-if="company.other_info_row2">
                                        <div class="m-0 p-0">
                                            <t t-esc="company.other_info_row2"/>
                                        </div>
                                    </li>
                                    <li style="font-size:9px" t-if="company.other_info_row3">
                                        <div class="m-0 p-0">
                                            <t t-esc="company.other_info_row3"/>
                                        </div>
                                    </li>
                                    <li style="font-size:9px" t-if="company.other_info_row4">
                                       <div class="m-0 p-0">
                                            <t t-esc="company.other_info_row4"/>
                                        </div>
                                    </li>
                                    <li style="font-size:9px" t-if="company.other_info_row5">
                                       <div class="m-0 p-0">
                                            <t t-esc="company.other_info_row5"/>
                                        </div>
                                    </li>
                                </ul>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </template>

        <template id="report_invoice_small_document">
                <t t-call="0043_account.external_layout">
                    <div class="article">
                        <div class="col-12" style="padding:0;margin:0;">
                            <table width="100%" cellspacing="0" cellpadding="0">
                                <thead>
                                    <tr>
                                        <th width="3%" style="font-size:10px;border-top: 1px solid black;border-bottom: 1px solid black;border-collapse: collapse; padding:0;margin:0;">
                                            No
                                        </th>
                                        <th width="15%" style="font-size:10px;border-top: 1px solid black;border-bottom: 1px solid black;border-collapse: collapse; padding:0;margin:0;">
                                            Kode Item
                                        </th>
                                        <th width="7%" style="font-size:10px;border-top: 1px solid black;border-bottom: 1px solid black;border-collapse: collapse; padding:0;margin:0;">
                                            Lot
                                        </th>
                                        <th style="font-size:10px;border-top: 1px solid black;border-bottom: 1px solid black;border-collapse: collapse; padding:0;margin:0;">
                                            Nama Item
                                        </th>
                                        <th width="5%" style="font-size:10px;border-top: 1px solid black;border-bottom: 1px solid black;border-collapse: collapse; padding:0;margin:0;">
                                           Jml
                                        </th>
                                        <th width="10%" class="text-right" style="font-size:10px; border-top: 1px solid black;border-bottom: 1px solid black;border-collapse: collapse; padding:0;margin:0;">
                                            Hrg-Sat
                                        </th>
                                        <th width="14%" class="text-right" style="font-size:10px; border-top: 1px solid black;border-bottom: 1px solid black;border-collapse: collapse; padding:0;margin:0;">
                                            Total
                                        </th>
                                    </tr>
                                </thead>
                                <tbody class="invoice_tbody">
                                    <t t-set="no" t-value="0"/>
                                    <t t-set="rowrepeat" t-value="0"/>
                                    <t t-set="rowsubtotal" t-value="0"/>
                                    <t t-set="quantity" t-value="0"/>
                                    <t t-foreach="o.invoice_line_ids.sorted(key=lambda r: r.product_id.name)" t-as="l">
                                        <t t-if="l.quantity != 0">
                                            <t t-set="no" t-value="no + 1"/>
                                            <t t-set="rowrepeat" t-value="rowrepeat + 1"/>
                                            <tr>
                                                <td style="white-space:nowrap;">
                                                    <div class="m-0 p-0" style="font-size:10px">
                                                        <t t-esc="no"/>
                                                    </div>
                                                </td>
                                                <td style="white-space:nowrap;">
                                                    <div class="m-0 p-0" style="font-size:10px"> 
                                                        <t t-esc="l.product_id.default_code"/>
                                                    </div>
                                                </td>
                                                <td style="white-space:nowrap;">
                                                    <div class="m-0 p-0" style="font-size:10px"/>
                                                </td>
                                                <td style="overflow:hidden;white-space:nowrap;">
                                                    <div class="m-0 p-0" style="font-size:10px">
                                                        <t t-esc="l.product_id.name"/> - <t t-esc="l.product_id.warna"/> - <t t-esc="l.product_id.motif"/> - <t t-esc="l.product_id.page"/>
                                                    </div>
                                                </td>
                                                <td style="white-space:nowrap;">
                                                    <div class="m-0 p-0" style="font-size:10px">
                                                        <t t-esc="int(l.quantity)"/> <t t-esc="l.uom_id.name"/>
                                                        <t t-set="quantity" t-value="quantity + l.quantity"/>
                                                    </div>
                        
                                                </td>
                                                <td style="white-space:nowrap;" class="text-right">
                                                    <div class="m-0 p-0" style="font-size:10px">
                                                        <span t-field="l.price_unit"/>
                                                    </div>
                                                </td>

                                                <td style="white-space:nowrap;" class="text-right">
                                                    <div class="m-0 p-0" style="font-size:10px">
                                                        <span t-field="l.price_total"/>
                                                    </div>
                                                    <t t-set="rowsubtotal" t-value="rowsubtotal + l.price_total"/>
                                                </td>
                                            </tr>
                                        </t>
                                    </t>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </t>
        </template>

        <template id="report_invoice_small">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-set="lang" t-value="o.user_id.sudo().lang if o.type in ('in_invoice', 'in_refund') else o.partner_id.lang"/>
                    <t t-call="0043_account.report_invoice_small_document" t-lang="lang"/>
                </t>
            </t>
        </template>

    </data>
</odoo>