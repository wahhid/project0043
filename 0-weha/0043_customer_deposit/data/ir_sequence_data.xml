<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_customer_deposit" model="ir.sequence">
            <field name="name">Customer Deposit</field>
            <field name="code">customer.deposit</field>
            <field name="prefix">CD</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_customer_deposit_payment" model="ir.sequence">
            <field name="name">Customer Deposit Payment</field>
            <field name="code">customer.deposit.payment</field>
            <field name="prefix">CDP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>