        <template id="report_picking_custom">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="0043_stock.picking_external_layout">
                        <style>
                            thead { display: table-header-group }
                            tfoot { display: table-row-group }
                            tr { page-break-inside: avoid }
                        </style>
                        <div class="page">
                            <div class="row">
                                <table width="100%">
                                  <thead>
                                    <tr style="border-top:0.05pt solid black;border-bottom:0.05pt solid black;">
                                        <th style="text-align:left"><span style="font-size:9px"><strong>No</strong></span></th>
                                        <th style="text-align:left"><span style="font-size:9px"><strong>Kode Item</strong></span></th>
                                        <th style="text-align:left">
                                            <span style="text-align:left;font-size:9px"><strong>Lot</strong></span>
                                        </th>
                                        <th style="text-align:left"><span style="font-size:9px"><strong>Nama Item</strong></span></th>
                                        <th style="text-align:right"><span style="font-size:9px"><strong>Jumlah</strong></span></th>
                                        <t t-if="o.picking_type_id.code != 'outgoing'"><th><span style="font-size:9px"><strong>Destination</strong></span></th></t>
                                    </tr>
                                  </thead>
                                    <tbody>
                                        <t t-set="no" t-value="0"/>
                                        <t t-set="quantity" t-value="0"/>
                                        <tr t-foreach="o.move_ids_without_package.sorted(key=lambda r: r.product_id.name)" t-as="move_id">
                                            <t t-set="no" t-value="no + 1"/>
                                            <td style="text-align:left"><span style="font-size:9px" t-esc="no"/></td>
                                            <td style="text-align:left">
                                                <span style="text-align:left;font-size:9px" t-field="move_id.product_id.default_code"/>
                                            </td>
                                            <td style="text-align:left">
                                                <span class="text-center" style="font-size:9px" t-if="move_id.line_lot_name" t-field="move_id.line_lot_name"/>
                                            </td>
                                            <td style="text-align:left">
                                                <span style="font-size:9px" t-field="move_id.product_id.name"/> -
                                                <span style="font-size:9px" t-field="move_id.product_id.merk_id"/> -
                                                <span style="font-size:9px" t-field="move_id.product_id.warna"/> -
                                                <span style="font-size:9px" t-field="move_id.product_id.page"/>
                                            </td>
                                            <td style="text-align:right">
                                                <t t-set="quantity" t-value="quantity + move_id.product_qty"/>
                                                <span style="font-size:9px" t-esc="int(move_id.product_qty)"/> 
                                                <span style="font-size:9px" t-field="move_id.product_uom"/>
                                            </td>
                                            <!--
                                            <t t-if="o.picking_type_id.code != 'outgoing'">
                                                <td><span t-field="move_id.location_dest_id"/>
                                                    <span style="font-size:9px" t-if="move_id.result_package_id">:</span>
                                                    <span style="font-size:9px" t-field="move_id.result_package_id"/>
                                                </td>
                                            </t>
                                            -->
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </t>
                 </t>
             </t>
        </template>

        <template id="report_picking_custom_dropshipping">
            <t t-call="report.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="0043_stock.dropshipping_external_layout">
