<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="picking_external_layout">
            <!-- Multicompany -->
            <t t-if="o and 'company_id' in o">
                <t t-set="company" t-value="o.company_id"></t>
            </t>
            <t t-if="not o or not 'company_id' in o">
                <t t-set="company" t-value="res_company"></t>
            </t>
            <t t-call="0043_stock.picking_external_layout_header"/>
            <t t-raw="0" />
            <t t-call="0043_stock.picking_external_layout_footer"/>
        </template>

        <template id="dropshipping_external_layout">
            <!-- Multicompany -->
            <t t-if="o and 'company_id' in o">
                <t t-set="company" t-value="o.company_id"></t>
            </t>
            <t t-if="not o or not 'company_id' in o">
                <t t-set="company" t-value="res_company"></t>
            </t>
            <t t-call="0043_stock.dropshipping_external_layout_header"/>
            <t t-raw="0" />
            <t t-call="0043_stock.picking_external_layout_footer"/>
        </template>

        <template id="checking_external_layout">
            <!-- Multicompany -->
            <t t-if="o and 'company_id' in o">
                <t t-set="company" t-value="o.company_id"></t>
            </t>
            <t t-if="not o or not 'company_id' in o">
                <t t-set="company" t-value="res_company"></t>
            </t>
            <t t-call="0043_stock.checking_external_layout_header"/>
            <t t-raw="0" />
            <t t-call="0043_stock.checking_external_layout_footer"/>
        </template>

        <template id="picking_external_layout_header">
            <div class="header">
                <div class="row">
                    <div class="col-6" style="padding:0;margin:0;">
                        <div class="row">
                            <div class="col-3">
                                <img src='/0043_stock/static/src/img/bcp_logo.jpeg' style="max-height: 60px;"/>
                            </div>
                            <div class="col-9">
                                <div class="row">
                                    <div class="col-12"  style="padding:0;margin:0;">
                                        <t t-if="not o.source_picking_id">
                                            <div class="m-0 p-0">
                                                <strong>SURAT JALAN</strong>
                                            </div>
                                        </t>
                                        <t t-if="o.source_picking_id">
                                            <div class="m-0 p-0">
                                                <strong>SURAT JALAN RETUR PENJUALAN</strong>
                                            </div>
                                        </t>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-12" style="padding:0;margin:0;">
                                        <div class="m-0 p-0" t-esc="company.name"/>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-12"  style="padding:0;margin:0;">
                                        <div class="m-0 p-0" style="font-size:9px" t-esc="company.street"/>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-12"  style="padding:0;margin:0;">
                                        <div class="m-0 p-0" style="font-size:9px">
                                            NPWP : <t t-esc="company.vat"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-12"  style="padding:0;margin:0;">
                                        <div class="m-0 p-0" style="font-size:9px">
                                            WA1 : <t t-esc="company.wa1"/> / WA2 : <t t-esc="company.wa2"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-12"  style="padding:0;margin:0;">
                                        <div class="m-0 p-0" style="font-size:9px">
                                            <t t-esc="o.create_uid.name"/> P-<t t-esc="o.printed_number"/> / <span class="page"/> of <span class="topage"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-5 col-offset-1">
                                <div t-if="o.move_lines and o.move_lines[0].partner_id and o.move_lines[0].partner_id.id != o.partner_id.id">
                                    <span><strong>Delivery Address:</strong></span>
                                    <div t-field="o.move_lines[0].partner_id"
                                         t-field-options='{"widget": "contact", "fields": ["address", "name", "phone", "fax"], "no_marker": true}'/>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-6" style="padding:0;margin:0;">
                        <div class="row">
                            <div class="col-4" style="padding:0;margin:0;">
                                <t t-if="not o.source_picking_id">
                                    <div class="m-0 p-0" style="font-size:9px">
                                        <strong>Tanggal SJ</strong>
                                    </div>
                                </t>
                                <t t-if="o.source_picking_id">
                                    <div class="m-0 p-0" style="font-size:9px">
                                        <strong>Tanggal Retur</strong>
                                    </div>
                                </t>
                            </div>
                             <div class="col-8" style="padding:0;margin:0;">
                                 <div class="m-0 p-0" style="font-size:9px" t-field="o.date"/>
                             </div>
                        </div>
                        <div class="row" t-if="o.source_picking_id">
                            <div class="col-4" style="padding:0;margin:0;">
                                <div class="m-0 p-0" style="font-size:9px"><strong>No Retur</strong></div>
                            </div>
                             <div class="col-8" style="padding:0;margin:0;">
                                 <div class="m-0 p-0" style="font-size:9px" t-field="o.name"/>
                             </div>
                        </div>
                        <div class="row" t-if="not o.source_picking_id">
                            <div class="col-4" style="padding:0;margin:0;">
                                <div class="m-0 p-0" style="font-size:9px"><strong>No SJ</strong></div>
                            </div>
                             <div class="col-8" style="padding:0;margin:0;">
                                 <div class="m-0 p-0" style="font-size:9px" >
                                    <span t-field="o.name"/> / <span t-field="o.sale_id.name"/>
                                 </div>
                             </div>
                        </div>
                        <div class="row" t-if="o.source_picking_id">
                            <div class="col-4" style="padding:0;margin:0;">
                                <div class="m-0 p-0" style="font-size:9px"><strong>No SJ</strong></div>
                            </div>
                             <div class="col-8" style="padding:0;margin:0;">
                                <div class="m-0 p-0" style="font-size:9px">
                                    <span t-field="o.origin"/> / <span t-field="o.sale_id.name"/>
                                </div>
                             </div>
                        </div>
                        <div class="row">
                            <div class="col-4" style="padding:0;margin:0;">
                                <div class="m-0 p-0" style="font-size:9px"><strong>Pelanggan</strong></div>
                            </div>
                            <div class="col-8" style="padding:0;margin:0;">
                                <div class="m-0 p-0" style="font-size:9px" t-field="o.partner_id.name"/>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-4" style="padding:0;margin:0;">
                                <div class="m-0 p-0" style="font-size:9px"><strong>Alamat Kirim</strong></div>
                            </div>
                            <div class="col-8" style="padding:0;margin:0;">
                                <t t-if="o.partner_id.street">
                                    <div class="m-0 p-0" style="font-size:9px" t-esc="o.partner_id.street"/>
                                </t>
                            </div>
                        </div>
                        <div class="row">
                                <div class="col-4" style="padding:0;margin:0;">
                                    <div class="m-0 p-0" style="font-size:9px"><strong>Kota / Provinsi</strong></div>
                                </div>
                                <div class="col-8" style="padding:0;margin:0;">
                                    <div class="m-0 p-0" style="font-size:9px">
                                    <t t-if="o.partner_id.city">
                                        <span t-field="o.partner_id.city"/>/ 
                                    </t>
                                    <t t-if="o.partner_id.state_id">
                                        <span t-field="o.partner_id.state_id"/>
                                    </t>
                                    </div>
                                </div>
                        </div>
                    </div>
                </div>
            </div>
        </template>

