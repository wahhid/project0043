<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="0043_customer_deposit_product_view_account_journal_form" model="ir.ui.view">
            <field name="name">0043.customer.deposit.product.view.account.journal.form</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='group_alias']" position="before">
                    <group string="Deposit Journal">
                        <field name="is_deposit"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>

